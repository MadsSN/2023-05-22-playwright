{
  "projectType": "library",
  "sourceRoot": "libs/user/src",
  "prefix": "eternal",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/user"],
      "options": {
        "jestConfig": "libs/user/jest.config.ts",
        "passWithNoTests": true
