    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/user/src/**/*.ts", "libs/user/src/**/*.html"]
      }
    }
  },
  "tags": ["domain:users", "type:feature"]
}
