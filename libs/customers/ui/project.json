    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/customers/ui/src/**/*.ts",
          "libs/customers/ui/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:customers", "type:ui"]
}
